<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.blogspot.ketikanmd.appcatatanagenda">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/agenda"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.blogspot.ketikanmd.agenda.TampilData"></activity>
        <activity
            android:name="com.blogspot.ketikanmd.agenda.CatatAgenda"
            android:label="Pencatat Agenda" />
        <activity android:name="com.blogspot.ketikanmd.agenda.LoginActivity" />
        <activity android:name="com.blogspot.ketikanmd.agenda.MainActivity" />
        <activity android:name="com.blogspot.ketikanmd.agenda.Splashscreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.blogspot.ketikanmd.agenda.RegisterActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name="com.blogspot.ketikanmd.agenda.DaftarAgenda" />
        <activity android:name="com.blogspot.ketikanmd.agenda.AgendaPenting" />
        <activity android:name="com.blogspot.ketikanmd.agenda.TampilAgenda" />
        <activity android:name="com.blogspot.ketikanmd.agenda.About" />
    </application>

</manifest>